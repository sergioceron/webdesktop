<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>

    <servlet>
        <servlet-name>FSService</servlet-name>
        <servlet-class>
            com.dotrow.projects.webdesktop.server.FileSystemServiceImpl
        </servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FSService</servlet-name>
        <url-pattern>
            /com.dotrow.projects.webdesktop.Main/filesystemservice
        </url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>welcomeGWT.html</welcome-file>
    </welcome-file-list>

</web-app>
